def prime(number):        
    if(number==1):   
        return False
    elif(number==2): 
        return True
    else:
        for x in range(2,number):  
            if(number%x==0):            
                return False
        return True
number=int(input("Enter the number"))
if(prime(number)):
    print("{} is a Prime number".format(number))
else:
    print("{} is not a Prime number".format(number))
